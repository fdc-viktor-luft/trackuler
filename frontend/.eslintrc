{
  "extends": [
    "react-app",
    "prettier"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-no-bind": "warn",
    "arrow-body-style": ["error", "as-needed"],
    "no-console": "warn",
    "eqeqeq": ["error", "always"],
    "no-duplicate-imports": 2
  }
}
