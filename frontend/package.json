{
  "name": "Trackula-Frontend",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://fdc-viktor-luft.github.io/trackuler/",
  "proxy": "http://localhost:7777",
  "dependencies": {
    "idb-keyval": "^5.0.6",
    "persistore": "^1.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "react-use-sub": "^2.1.0"
  },
  "scripts": {
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "build:docs": "rm -rf ../docs && pnpm build && mv build ../docs",
    "test": "react-scripts test --color",
    "start": "PORT=7776 react-scripts start",
    "eslint": "eslint --cache --cache-location 'misc/.eslintcache' --ext .ts,.tsx src/",
    "circular-check": "madge -c --extensions ts,tsx --ts-config tsconfig.json --no-spinner src/"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "madge": "^4.0.2",
    "prettier": "^2.3.0",
    "sass": "^1.34.0",
    "spy4js": "^3.1.1",
    "typescript": "4.3.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
