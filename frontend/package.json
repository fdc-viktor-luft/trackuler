{
  "name": "Trackuler-Frontend",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "homepage": "https://fdc-viktor-luft.github.io/trackuler/",
  "proxy": "http://localhost:7777",
  "scripts": {
    "build": "vite build",
    "build:docs": "rm -rf ../docs && pnpm build && mv build ../docs",
    "test": "vitest --run --color",
    "dev": "vite dev",
    "lint": "pnpm lint:circular-imports && pnpm lint:es && pnpm lint:ts",
    "lint:es": "eslint --ext .ts,.tsx src --max-warnings 0",
    "lint:ts": "tsc",
    "lint:circular-imports": "madge -c --extensions ts,tsx --ts-config tsconfig.json --no-spinner src/"
  },
  "dependencies": {
    "dompurify": "^3.0.0",
    "idb-keyval": "^6.2.0",
    "marked": "^4.2.12",
    "morfi": "^2.0.0",
    "persistore": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use-sub": "^3.0.1"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/dompurify": "^2.4.0",
    "@types/marked": "^4.0.8",
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^21.1.0",
    "madge": "^6.0.0",
    "prettier": "^2.8.4",
    "sass": "^1.58.3",
    "spy4js": "^3.4.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.3",
    "vitest": "^0.28.5"
  }
}
