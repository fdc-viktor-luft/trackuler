{
  "name": "Trackula-Frontend",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://fdc-viktor-luft.github.io/trackuler/",
  "proxy": "http://localhost:7777",
  "dependencies": {
    "idb-keyval": "^5.0.6",
    "morfi": "^1.1.9",
    "persistore": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "react-use-sub": "^2.2.0"
  },
  "scripts": {
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "build:docs": "rm -rf ../docs && pnpm build && mv build ../docs",
    "test": "react-scripts test --color",
    "start": "PORT=7776 react-scripts start",
    "eslint": "eslint --cache --cache-location 'misc/.eslintcache' --ext .ts,.tsx src/",
    "circular-check": "madge -c --extensions ts,tsx --ts-config tsconfig.json --no-spinner src/"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.0",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "madge": "^5.0.1",
    "prettier": "^2.3.2",
    "sass": "^1.35.2",
    "spy4js": "^3.1.3",
    "typescript": "4.3.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
